{
  "name": "@shilong/react",
  "version": "0.0.1",
  "packageManager": "pnpm@10.6.3",
  "description": "A simple react ui component",
  "keywords": [
    "ui",
    "react",
    "tailwindcss",
    "shadcn"
  ],
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/Onlylonger/react",
  "bugs": {
    "url": "https://github.com/Onlylonger/react/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Onlylonger/react.git"
  },
  "author": "Barry <onlylonger@outlook.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/global.css",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build -w",
    "pg": "vite --config playground/vite.config.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "pnpm build && bumpp && npm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.4",
    "@vitejs/plugin-react": "^4.4.1",
    "bumpp": "^10.1.0",
    "happy-dom": "^17.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.8.3",
    "vite": "^7.1.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.1.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.15",
    "@shilong/utils": "^0.0.9",
    "sonner": "^2.0.7"
  },
  "pnpm": {
    "overrides": {
      "@shilong/ui": "link:"
    }
  }
}
